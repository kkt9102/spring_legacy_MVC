<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd.mybatis-3-mapper.dtd">
	
	<mapper namespace="com.crud.mapper.ArticleMapper">
	<!-- 게시글 작성 -->
	<insert id="doWrite">
		INSERT INTO crud_article
		SET title = #{title},
		body = #{body},
		writer = #{writer}
	</insert>
	</mapper>